{
  "name": "@transferto/shared",
  "version": "0.1.0",
  "private": true,
  "main": "dist/index.ts",
  "module": "dist/index.ts",
  "types": "dist/index.d.ts",
  "scripts": {
    "clean": "rimraf --no-glob ./tsconfig.tsbuildinfo",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "typecheck": "tsc --project ./tsconfig.json --noEmit",
    "test": "run-s 'test:*'",
    "test:unit": "echo \"No tests yet\"",
    "fix:staged-files": "lint-staged --allow-empty",
    "fix:all-files": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "pre-commit": "echo 'Shared precomit running...",
    "start": "rm -rf dist && rollup -c rollup.config.ts",
    "dev": "rollup -c rollup.config.ts -w"
  },
  "devDependencies": {
    "@babel/core": "^7.18.5",
    "@storybook/addon-actions": "^6.5.8",
    "@storybook/addon-essentials": "^6.5.8",
    "@storybook/addon-interactions": "^6.5.8",
    "@storybook/addon-links": "^6.5.8",
    "@storybook/builder-webpack5": "^6.5.8",
    "@storybook/manager-webpack5": "^6.5.8",
    "@storybook/react": "^6.5.8",
    "@storybook/testing-library": "^0.0.13",
    "@types/node": "^17.0.41",
    "@types/react": "^18.0.12",
    "babel-loader": "^8.2.5",
    "eslint-config-prettier": "^8.5.0",
    "prettier": "^2.7.1"
  },
  "dependencies": {
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@lifi/wallet-management": "^1.1.10",
    "@mui/material": "^5.8.3",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "11.0.2",
    "@rollup/plugin-image": "^2.1.1",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^11.1.1",
    "i18next": "^21.9.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^11.18.6",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-typescript2": "^0.31.1",
    "typescript": "^4.7.3",
    "use-media": "^1.4.0"
  }
}
